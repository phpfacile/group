{
    "name": "phpfacile/group",
    "description": "Interface of services to manage items (of any kind) that can be grouped (by any manner).",
    "keywords": ["group", "interface"],
    "homepage": "https://github.com/phpfacile/group",
    "license": "MIT",
    "authors": [
        {
            "name": "PHP Facile!",
            "homepage": "https://www.phpfacile.com/"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "psr/log": "^1.0.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*"
    },
    "scripts": {
        "cs-check": "phpcs -s src",
        "cs-check-psr": "phpcs -s --standard=PSR1,PSR2 src",
        "cs-check-pear": "phpcs -s --standard=PEAR src",
        "cs-fix": "phpcbf src"
    },
    "autoload": {
        "psr-4": {
          "PHPFacile\\Group\\": "src/"
        }
    }
}
